msgstr ""

#. type: delimited block =
#: documentation/content/en/books/porters-handbook/special/chapter.adoc:4052
msgid "|=== | Database | USES Macro"
msgstr ""

#. type: delimited block =
#: documentation/content/en/books/porters-handbook/special/chapter.adoc:4055
msgid "|Berkeley DB |<<uses-bdb,`bdb`>>"
msgstr ""

#. type: delimited block =
#: documentation/content/en/books/porters-handbook/special/chapter.adoc:4058
msgid "|MariaDB, MySQL, Percona |<<uses-mysql,`mysql`>>"
msgstr ""

#. type: delimited block =
#: documentation/content/en/books/porters-handbook/special/chapter.adoc:4061
msgid "|PostgreSQL |<<uses-pgsql,`pgsql`>>"
msgstr ""

#. type: delimited block =
#: documentation/content/en/books/porters-handbook/special/chapter.adoc:4065
msgid "|SQLite |<<uses-sqlite,`sqlite`>> |==="
