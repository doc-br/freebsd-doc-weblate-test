#: documentation/content/en/articles/committers-guide/_index.adoc:2086
msgid "<<developer.relations>>"
msgstr ""

#. type: Plain text
#: documentation/content/en/articles/committers-guide/_index.adoc:2087
msgid "<<ssh.guide>>"
msgstr ""

